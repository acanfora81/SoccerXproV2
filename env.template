# SoccerXpro V2 - Environment Variables

# Node Environment
NODE_ENV=development

# Server Configuration
PORT=3001

# Database Configuration
DATABASE_URL="postgresql://postgres:password@localhost:5432/soccerxpro_v2"

# Supabase Configuration (placeholder values - replace with real ones)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Redis Configuration (optional - will use fallback if not available)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
REDIS_URL=

# Import Configuration
IMPORT_BATCH_SIZE=50
IMPORT_TX_TIMEOUT_MS=60000
IMPORT_BATCH_DELAY_MS=100
IMPORT_DEBUG=1

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# CORS Configuration
CORS_ORIGIN=http://localhost:5173

# Feature Flags
FEATURE_MARKET_MODULE=true